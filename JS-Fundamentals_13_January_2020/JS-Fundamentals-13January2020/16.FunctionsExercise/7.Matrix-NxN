function matrix(numbers){

    let matrixOfNumber = '';

    for(let i = 0 ; i <numbers ; i++){
    
        for(let j = 0; j <numbers ;j++){
        
            matrixOfNumber += numbers + ' ';
        
        }
        
        console.log(matrixOfNumber)
        matrixOfNumber = ''
    }
}
matrix(7)
