function main(firstNum,secondNum,thirdNum){
    let result = 0 ;
    if(firstNum <= secondNum && firstNum <= thirdNum){
        result = firstNum
    }
    if(secondNum <= firstNum && secondNum <= thirdNum){
        result = secondNum
    }
    if(thirdNum <= secondNum && thirdNum <= firstNum){
        result = thirdNum
    }
    console.log(result)

};
main(2,5,3)
